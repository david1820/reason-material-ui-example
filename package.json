    "bs-json": "github:buckletypes/bs-json",
    "bs-platform": "^1.8.1",
    "material-ui": "^0.18.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "reason-react": ">=0.2.1",
    "webpack": "^1.14.0"
